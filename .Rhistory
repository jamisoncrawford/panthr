# DEMOGRAPHICS FAMILY: decode_race()
decode_race <- function(code){
x <- code
y <- vector()
for (i in seq_along(x)){
na <- is.na(x[i])
rc <- x[i]
if (na){y[i] <- "Not Reported"}
else if (!na & grepl("[AC-HJ-OQ-VX-Y03-9]{,1}", rc)){
y[i] <- NA
